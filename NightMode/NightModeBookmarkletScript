javascript:(
  function () {
    if (document.getElementById('nightModeScript') == null) {
      var js = document.createElement('script');
      js.setAttribute('type', 'text/javascript');
      jq.setAttribute('id', 'nightModeScript');
      js.setAttribute('src', 'https://raw.githubusercontent.com/thislooksfun/YouTubeNightMode/master/NightMode/JavaScript/NightMode.js');
      document.head.appendChild(js);
    }
 }()
