javascript:(
  function () {
    if (document.getElementById('nightModeScript') == null) {
      var js = document.createElement('script');
      js.setAttribute('type', 'text/javascript');
      js.setAttribute('id', 'nightModeScript');
      js.setAttribute('src', 'https://raw.githubusercontent.com/thislooksfun/YouTubeNightMode/master/NightMode.js');
      document.head.appendChild(js);
    }
    
    if (document.getElementById('dimmerScript') == null) {
      var js = document.createElement('script');
      js.setAttribute('type', 'text/javascript');
      js.setAttribute('id', 'dimmerScript');
      js.setAttribute('src', 'https://raw.githubusercontent.com/thislooksfun/YouTubeDimmer/master/Dimmer.js');
      document.head.appendChild(js);
    }
 }()
);
